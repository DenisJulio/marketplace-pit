package components

templ Hello(name string) {
	<!DOCTYPE html>
	<html lang="en">
		<head>
			<meta charset="UTF-8"/>
			<meta name="viewport" content="width=device-width, initial-scale=1.0"/>
			<title>Document</title>
            <link rel="stylesheet" href="static/style.css">
		</head>
		<body>
			<div class="text-red-500">Hello, { name }</div>
		</body>
	</html>
}
